#!/bin/sh

export CFLAGS="$CFLAGS -static --static"
export LDFLAGS="$LDFLAGS -static --static"

./configure \
    --prefix="$_prefix"

make DESTDIR="$1" install

ln -s bsdtar "$1/$_prefix/bin/tar"
ln -s bsdcpio "$1/$_prefixbin/cpio"
